            <th title="Release Penalty">PEN</th>
            <th title="Release Net Savings">SAV</th>
            <th title="Years Left on Current Contract">YRS</th>
        </tr>
        @foreach (var player in Model.Where(m => m.Position == "QB").OrderByDescending(m => m.Overall))
        {
            <tr>
                <td>@player.Position</td>
                <td>@player.FirstName.Substring(0, 1) @player.LastName</td>
                <td>@player.Age</td>
                <td>@player.Overall</td>
