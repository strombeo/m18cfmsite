            <th>Name</th>
            <th>Age</th>
            <th title="Overall">OVR</th>
            <th title="Speed">SPD<br />5%</th>

            <th title="Throw Power">THP<br />26%</th>

            <th title="Throw Accuracy Short">TAS<br />18%</th>
